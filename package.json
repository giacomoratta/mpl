{
  "name": "mpl",
  "version": "0.1.0",
  "description": "Creativity-oriented music production library manager",
  "main": "app.js",
  "scripts": {
    "build": "node build.js",
    "test": "jest --testPathPattern=.*\\__tests__\\.*\\.test\\.js",
    "test-win": "jest --testPathPattern=.*/__tests__/.*.test.js",
    "start": "NODE_ENV=production APP_DATA_DIRNAME=app-data node app.js",
    "start-win": "set NODE_ENV=production&& set APP_DATA_DIRNAME=app-data&& node app.js",
    "dev": "NODE_ENV=development APP_DATA_DIRNAME=test-app-data node app.js",
    "dev-win": "set NODE_ENV=development&& set APP_DATA_DIRNAME=test-app-data&& node app.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^3.1.1",
    "fastest-validator": "^1.1.0",
    "fs-extra": "^9.0.0",
    "iconv-lite": "^0.5.1",
    "lodash": "^4.17.15",
    "pino": "^6.0.0",
    "pino-pretty": "^4.0.0",
    "pino-tee": "^0.3.0",
    "rimraf": "^3.0.2",
    "symbol-tree": "^3.2.4",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "jest": "^25.2.4",
    "nexe": "^3.3.2",
    "standard": "^14.3.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      ".example."
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "expect",
      "it",
      "beforeEach",
      "beforeAll",
      "afterEach",
      "afterAll"
    ]
  }
}
